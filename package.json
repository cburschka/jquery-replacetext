{
  "name": "jquery-replacetext",
  "version": "1.1.0",
  "description": "Replace substrings inside DOM nodes with text or new nodes.",
  "main": "replacetext.js",
  "babel": {
    "presets": ["es2015"]
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1",
    "jquery": ">=1.0"
  },
  "scripts": {
    "test": "$('<span>jQuery</span>').replaceText(/jQuery/, $('<a href=\"https://jquery.com/\">jQuery</a>'));"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cburschka/jquery-replacetext.git"
  },
  "keywords": [
    "jquery",
    "replace",
    "text",
    "jquery-plugin",
    "ecosystem:jquery"
  ],
  "author": "Christoph Burschka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cburschka/jquery-replacetext/issues"
  },
  "homepage": "https://github.com/cburschka/jquery-replacetext#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0"
  }
}
